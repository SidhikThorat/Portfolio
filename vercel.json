{
  "rewrites": [
    {
      "source": "/resume",
      "destination": "/Sidhik_Thorat_Resume.pdf"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/Sidhik_Thorat_Resume.pdf",
      "headers": [
        {
          "key": "Content-Disposition",
          "value": "attachment; filename=\"Sidhik_Thorat_Resume.pdf\""
        }
      ]
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite"
}
